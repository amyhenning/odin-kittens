<% @kittens.each do |kitten| %>
	<h1><%= link_to kitten.name, kitten_path(kitten.id) %></h1>
	<p>Age: <%= kitten.age %></p>
	<p>Cuteness: <%= kitten.cuteness %></p>
	<p>Softness: <%= kitten.softness %></p>
	<p><%= link_to "Delete", kitten_path(kitten), method: :delete, data: {confirm: 'Are you sure you want to delete this kitten?'} %></p>
<% end %>